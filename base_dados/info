CREATE TABLE `PESSOAS` (
    `ID` INT  NOT NULL ,
    `CPF` TEXT  NOT NULL ,
    `NOME` TEXT  NOT NULL ,
    `IDADE` INT  NOT NULL ,
    PRIMARY KEY (
        `ID`
    )
);

# VERS√ÉO SQLITE

CREATE TABLE PESSOAS (
    ID INTEGER PRIMARY KEY AUTOINCREMENT ,
    CPF TEXT ,
    NOME TEXT NOT NULL ,
    IDADE INTEGER
)

